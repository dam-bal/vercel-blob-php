{
    "name": "dam-bal/vercel-blob-php",
    "description": "PHP Client for Vercel Blob",
    "type": "library",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": {
            "VercelBlobPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\VercelBlobPhp\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Damian Balandowski"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
